
.name-list {
  width: 100%;
  height: 1000px;
  top: 40px;
  box-sizing: border-box;
  position: relative;
  /* z-index: 1025; */
}
.item {
  line-height: 40rpx;
  padding: 30rpx 50rpx;
  text-align: left;
  border-top: 1px solid #eee;
}
.name {
  color: #282828;
  font-size: 32rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.add {
  color: #707070;
  font-size: 24rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.noData {
  width: 100%;
  height: 100px;
  top: 80px;
  box-sizing: border-box;
  position: relative;
  display: flex;
  justify-content: center;
}
.content{
  font-size: 40rpx;
}
