/* pages/mine/mine.wxss */
.UCenter-bg {
    /*第一张图不是太满意*/
    /* background-image: url(https://riyugo.com/i/2020/12/30/52c088r.jpg); */
    background-image: url(http://m.qpic.cn/psc?/V111Bxku4EYHMi/ruAMsa53pVQWN7FLK88i5uFjYiZEwtO3tXnwKqtknkpLZ1IG790x3fVQ0tYgoPhWCFEoLRWdshkAm67KwTsyPVrHgdHUDlx6KqFBnAHYzsU!/mnull&bo=mAL1AgAAAAABB00!&rf=photolist&t=5);
    background-size: cover;
    height: 550rpx;
    display: flex;
    justify-content: center;
    padding-top: 40rpx;
    overflow: hidden;
    position: relative;
    flex-direction: column;
    align-items: center;
    color: #fff;
    font-weight: 300;
    text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
  }
  
  .UCenter-bg text {
    opacity: 0.8;
  }
  
  .UCenter-bg image {
    width: 200rpx;
    height: 200rpx;
  }
  
  .UCenter-bg .gif-wave{
    position: absolute;
    width: 100%;
    bottom: 0;
    left: 0;
    z-index: 99;
    mix-blend-mode: screen;  
    height: 100rpx;   
  }
  map,.mapBox{
    left: 0;
    z-index: 99;
    mix-blend-mode: screen;  
    height: 100rpx;   
  }
  
  map,.mapBox{
    width: 750rpx;
    height: 300rpx;
  }